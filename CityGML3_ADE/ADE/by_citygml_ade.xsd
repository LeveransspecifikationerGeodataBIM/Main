    <restriction base="string">
      <enumeration value="länsstyrelse">
        <annotation>
          <documentation>-- Definition --
under uppförande

-- Beskrivning --
Godkänt bygglov finns, byggnaden planeras att uppföras i närtid eller på längre sikt.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="Sjöfartsverket"/>
      <enumeration value="Naturvårdsverket">
        <annotation>
          <documentation>-- Definition --
inte längre gällande/levande.

-- Beskrivning --
Byggnaden har t.ex. rivits eller brunnit ned.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="annan namngivare"/>
      <enumeration value="Lantmäteriet"/>
      <enumeration value="Trafikverket"/>
