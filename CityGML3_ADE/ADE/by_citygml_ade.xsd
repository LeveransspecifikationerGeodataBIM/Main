  <simpleType name="BY_ByggnadStatusTypEnumerationType">
    <annotation>
      <documentation>-- Definition --
aktuell status för byggnad</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="planerad">
        <annotation>
          <documentation>-- Definition --
under uppförande

-- Beskrivning --
Godkänt bygglov finns, byggnaden planeras att uppföras i närtid eller på längre sikt.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="gällande">
        <annotation>
          <documentation>-- Definition --
färdigställd och uppförd

-- Beskrivning --
Byggnaden är uppförd och slutbesked har beslutats.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="avregistrerad">
        <annotation>
          <documentation>-- Definition --
inte längre gällande/levande.

-- Beskrivning --
Byggnaden har t.ex. rivits eller brunnit ned.</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="BY_ByggnadStatusTypOtherType">
    <restriction base="string">
      <pattern value="other: \w{2,}"/>
    </restriction>
  </simpleType>
  <simpleType name="BY_ByggnadTillbehorTypType">
    <annotation>
      <documentation>-- Definition --
typ av konstruktion, generellt liten, som är ihopbyggd med en byggnad

-- Exempel --
balkong, mast, skorsten, trappa

.</documentation>
    </annotation>
    <union memberTypes="byade:BY_ByggnadTillbehorTypEnumerationType byade:BY_ByggnadTillbehorTypOtherType"/>
  </simpleType>
  <simpleType name="BY_ByggnadTillbehorTypEnumerationType">
    <annotation>
      <documentation>-- Definition --
typ av konstruktion, generellt liten, som är ihopbyggd med en byggnad

-- Exempel --
balkong, mast, skorsten, trappa

.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="altan">
        <annotation>
          <documentation>-- Definition --
iordningsställd begränsad yta i anslutning till bostaden
&lt;i&gt;Källa: TNC, Terminologicenter&lt;/i&gt;</documentation>
        </annotation>
      </enumeration>
      <enumeration value="balkong">
        <annotation>
          <documentation>-- Definition --
med skyddsräcke försedd platta som ofta skjuter ut från väggliv
&lt;i&gt;Källa: TNC, Terminlogicenter&lt;/i&gt;</documentation>
        </annotation>
      </enumeration>
      <enumeration value="brandvägg">
        <annotation>
          <documentation>-- Definition --
vägg med särskilt utförande som anordnas till skydd mot brandspridning mellan byggnader eller delar av en större byggnad

&lt;i&gt;Källa: Rikstermbanken, TNC, Terminologicentrum, Plan och byggtermer &lt;/i&gt;

&lt;i&gt;Anmärkning:&lt;/i&gt; Termen Brandvägg ersätter de tidigare använda termerna brandmur och brandsäker mur.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="lastbrygga">
        <annotation>
          <documentation>-- Definition --
anordning i form av ett stationärt eller flyttbart plan avsett att vid lastning och lossning utjämna nivåskillnad mellan lastkaj och fordons lastutrymme
&lt;i&gt;Källa: TNC, Terminologicentrum&lt;/i&gt;</documentation>
        </annotation>
      </enumeration>
      <enumeration value="mast">
        <annotation>
          <documentation>-- Definition --
vertikal stående konstruktion på byggnad</documentation>
        </annotation>
      </enumeration>
      <enumeration value="skorsten">
        <annotation>
          <documentation>-- Definition --
vertikal rörformad konstruktion på byggnad för att leda bort rökgaser
&lt;i&gt;
&lt;/i&gt;</documentation>
        </annotation>
      </enumeration>
      <enumeration value="takfönster">
        <annotation>
          <documentation>-- Definition --
vanligen i form av glasat ramverk, som täpper till öppning i vägg eller tak och som har till väsentlig funktion att släppa in dagsljus
&lt;i&gt;Källa: TNC, Terminologicentrum, Plan och Byggtermer&lt;/i&gt;</documentation>
        </annotation>
      </enumeration>
      <enumeration value="takkupa">
        <annotation>
          <documentation>-- Definition --
utskjutande parti av yttertak utbyggt i takfall och försett med (fönster)öppning framtill
&lt;i&gt;Källa: TNC, Terminologicentrum, Träbyggnadsordlista&lt;/i&gt;</documentation>
        </annotation>
      </enumeration>
      <enumeration value="trappa">
        <annotation>
          <documentation>-- Definition --
förbindelseled mellan olika nivåer bestående av trapplopp och eventuella vilplan
&lt;i&gt;Källa: TNC, Terminologicentrum, Plan- och Byggtermer&lt;/i&gt;

-- Beskrivning --</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="BY_ByggnadTillbehorTypOtherType">
    <restriction base="string">
      <pattern value="other: \w{2,}"/>
    </restriction>
  </simpleType>
